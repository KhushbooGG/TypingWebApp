@model IEnumerable<Institute>
@{
    ViewData["Title"] = "InstituteReport";
    Layout = "~/Views/Shared/_SuperAdminLayout.cshtml";
}

<h1>InstituteReport</h1>

<table class="table">
    <tr>
        <th>
            Institute Name
        </th>
        <th>
            Principal Name
        </th>
        <th>
            Institute Address
        </th>
        <th>
            Institute Code
        </th>
        <th>
            Contact No
        </th>
        <th>
            Email
        </th>
        <th>
            No Of Computer in Lab
        </th>
        <th>
            Status
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                
                @Html.DisplayFor(modelItem => item.InstituteName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PrincipalName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InstituteAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InstituteCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ContactNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NoOfComputer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
        </tr>
    }

</table>